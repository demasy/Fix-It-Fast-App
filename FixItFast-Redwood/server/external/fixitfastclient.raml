            item-path-name: incident-activity
            example-item: |
              {
                "id": "act-101",
                "firstName": "Ricky",
                "lastName": "Barker",
                "userId": "cus-101",
                "role": "Customer",
                "createdOn": "2015-07-18T09:00:41+00:00",
                "comment": "Fridge is utterly broken!",
                "picture": "data:image/png;base64,"
              }

            example-patchitem: |
              {
                "comment": "Fridge doesn't power up at all"
              }

