        "id": "incidentsStats",
        "title": "Incident Statistics",
        "type": "object",
        "description": "Incident counts by priority",
        "properties": {
          "incidentCount": {
            "type": "object",
            "description": "The number of incidents for each priority",
            "properties": {
              "high": {
                "type": "integer",
                "description": "The number of incidents with priority high"
              },
              "normal": {
                "type": "integer",
                "description": "The number of incidents with priority normal"
              },
              "low": {
                "type": "integer",
                "description": "The number of incidents with priority low"
              },
              "revNum": {
                "type": "integer",
                "description": "Attribute used for offline cases and incremented when data is updated on the server"
              }
            }
          }
        },
        "links": [
          {
           "rel": "self",
           "title": "Incidents Statistics",
           "href": "/stats/incidents",
           "method": "GET",
           "targetSchema": {"$ref": "incidentsStats"}
          }
        ]
      }

  - incidentGroup: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "incidentGroup",
        "title": "Incident Count Metric",
        "type": "object",
        "description": "Incident count for a single priority",
        "properties": {
          "priority": {
            "type": "string",
            "description": "The property used to group incidents by",
             "enum": [
                "low",
                "normal",
                "high"
