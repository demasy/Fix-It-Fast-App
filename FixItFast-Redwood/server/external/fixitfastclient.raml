resourceTypes:
  - collection:
      usage: collection with GET and optional POST endpoint
      description: List of <<path-name>>.
      get:
       description: Get a list of <<path-name>>.
       responses:
        200:
         body:
          application/json:
           schema: <<path-name>>
           example: <<example-collection>>
      post?:
       description: Create a new <<item-path-name>>
       body:
        application/json:
         schema: new-<<item-path-name>>
         example: <<example-newitem>>
       responses:
        201:
         headers:
          Location:
           description: The relative URL of the created item.
           type: string
           required: true
           example: /incidents/inc-345
         body:
          application/json:
           schema: <<item-path-name>>
           example: <<example-item>>

  - item:
      usage: singular item with GET and optional PUT, PATCH and DELETE endpoints
      description: A single <<item-path-name>>
      get:
       description: Get a single <<item-path-name>>.
       responses:
        200:
         body:
          application/json:
           schema: <<item-path-name>>
           example: <<example-item>>
        404:
         body:
          application/json:
           example: |
            {"message": "<<item-path-name>> not found" }
      patch?:
       description: Update a specific <<item-path-name>>.
       body:
        application/json:
         schema: patch-<<item-path-name>>
         example: <<example-patchitem>>
       responses:
        200:
         body:
          application/json:
           schema: <<item-path-name>>
           example: <<example-item>>
        404:
         body:
          application/json:
           example: |
            {"message": "Unable to find <<item-path-name>> record."}
      put?:
       description: Update a specific <<item-path-name>>.
       body:
        application/json:
         schema: patch-<<item-path-name>>
         example: <<example-patchitem>>
       responses:
        200:
         body:
          application/json:
           schema: <<item-path-name>>
           example: <<example-item>>
        404:
         body:
          application/json:
           example: |
            {"message": "Unable to find <<item-path-name>> record."}
      delete:
       description: Remove/close the specific <<item-path-name>>.
       responses:
        204:
         description: The specific <<item-path-name>> item was deleted successfully.

  - collectionWithFormPost:
      usage: collection with GET and optional POST endpoint with content type application/x-www-form-urlencoded.
      description: List of <<path-name>>.
      get:
       description: Get a list of <<path-name>>.
       responses:
        200:
         body:
          application/json:
           schema: <<path-name>>
           example: <<example-collection>>
      post?:
       description: Create/add a new <<item-path-name>>
       body:
        application/x-www-form-urlencoded:
       responses:
        201:
         headers:
          Location:
           description: The relative URL of the created item.
           type: string
           required: true
           example: /incidents/inc-345
         body:
          application/json:
           schema: <<item-path-name>>
           example: <<example-item>>

