                  {
                    "$schema": "http://json-schema.org/draft-04/schema#",
                    "id": "incident-activity",
                    "title": "Incident Activity",
                    "type": "object",
                    "description": "A single activity reported on an incident",
                    "properties": {
                        "firstName": {
                           "type": "string",
                           "description": "The first name of the person who created the activity"
                        },
                        "lastName": {
                           "type": "string",
                           "description": "The person's last name"
                        },
                        "userId": {
                           "type": "string",
                           "description": "The person's unique identifier"
                        },
                        "role": {
                           "type": "string",
                           "description": "The person's role, e.g. Customer or Field Service"
                        },
                        "comment": {
                            "type": "string",
                            "description": "A brief comment posted by the user",
