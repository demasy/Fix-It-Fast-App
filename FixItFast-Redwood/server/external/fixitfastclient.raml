#%RAML 0.8
title: FixItFastClient
version: 4.0
baseUri: /mobile/custom/fixitfastclient
protocols: [HTTPS]
mediaType: "application/json"
schemas:
  - incidents:      {
        "$schema": "http://json-schema.org/draft-04/hyper-schema#",
        "id": "incidents",
        "title": "Incidents Result",
        "type": "object",
        "properties": {
          "count": {
            "type": "integer",
            "description": "The number of incidents returned"
          },
          "result": {
            "type": "array",
            "items": {
              "$ref": "incident-summary"
            },
            "description": "List of incidents with summary information"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "links": [
          {
            "rel":          "self",
            "title":        "Incidents",
            "href":         "/incidents",
            "method":       "GET"
          },
          {
            "rel":          "create",
            "title":        "Create Incident",
            "href":         "/incidents",
            "method":       "POST",
            "schema":       {"$ref": "new-incident"},
            "targetSchema": {"$ref": "incident"}
          }
        ]
      }

  - incident-summary: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "incident",
        "title": "Incident Detail",
        "type": "object",
        "properties": {
          "id": {
             "type": "string",
             "description": "Unique incident ticket number"
          },
          "problem": {
             "type": "string",
             "description": "Short description of the incident"
          },
          "lastUpdatedOn": {
             "type": "string",
             "format": "date-time",
             "description": "When the incident was last updated"
          },
          "status": {
             "type": "string",
             "description": "Incident status: 1=open, 2=accepted, 3=fixed",
             "enum": [
                "open",
                "accepted",
                "fixed"
             ]
          },
          "priority": {
             "type": "string",
             "description": "Incident priority: 1=high, 2=normal, 3=low",
             "enum": [
                "low",
                "normal",
                "high"
             ]
          },
          "category": {
             "type": "string",
             "description": "Incident category: appliance, electrical, general, heatingcooling, plumbing",
             "enum": [
                "appliance",
                "electrical",
                "general",
                "heatingcooling",
                "plumbing"
             ]
          },
          "customer": {
            "$ref": "incident-customer",
            "title": "Details of customer who logged the incident"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "links": [
          {
            "rel": "canonical",
            "title": "Incident",
            "href": "/incidents/{id}",
            "method": "GET",
            "targetSchema": {"$ref": "incident"}
          },
          {
            "rel": "closeIncident",
            "title": "Close an incident",
            "href": "/incidents/{id}/closeIncident",
            "method": "POST"
          },
          {
            "rel": "delete",
            "title": "Delete an incident",
            "href": "/incidents/{id}",
            "method": "DELETE"
          },
          {
            "rel": "edit",
            "title": "Update an incident",
            "href": "/incidents/{id}",
            "method": "PUT",
            "mediaType": "application/json",
            "schema": {"$ref": "patch-incident"},
            "targetSchema": {"$ref": "incident"}
          },
          {
            "rel": "parent",
            "title": "Incidents",
            "href": "/incidents",
            "method": "GET",
            "mediaType": "application/json",
            "targetSchema": {"$ref": "incidents"}
          }
        ]
      }

  - incident: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "incident",
        "title": "Incident Detail",
        "type": "object",
        "properties": {
          "id": {
             "type": "string",
             "description": "Unique incident ticket number"
          },
          "problem": {
             "type": "string",
             "description": "Short description of the incident"
          },
          "description": {
             "type": "string",
             "description": "Detailed description of the incident"
          },
          "createdOn": {
             "type": "string",
             "format": "date-time",
             "description": "When the incident was logged"
          },
          "lastUpdatedOn": {
             "type": "string",
             "format": "date-time",
             "description": "When the incident was last updated"
          },
          "picture": {
            "type": "string",
            "media": {
                "binaryEncoding": "base64",
                "type": "image/png"
            },
            "description": "A picture uploaded to the incident activity"
          },
          "status": {
             "type": "string",
             "description": "Incident status: 1=open, 2=accepted, 3=fixed",
             "enum": [
                "open",
                "accepted",
                "fixed"
             ]
          },
          "priority": {
             "type": "string",
             "description": "Incident priority: 1=high, 2=normal, 3=low",
             "enum": [
                "low",
                "normal",
                "high"
             ]
          },
          "category": {
             "type": "string",
             "description": "Incident category: appliance, electrical, general, heatingcooling, plumbing",
             "enum": [
                "appliance",
                "electrical",
                "general",
                "heatingcooling",
                "plumbing"
             ]
          },
          "customer": {
            "$ref": "incident-customer",
            "title": "Details of customer who logged the incident"
          },
          "location": {
            "$ref": "IncidentLocation",
            "title": "Location where the incident occurred"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "links": [
          {
            "rel": "self",
            "title": "Incident",
            "href": "/incidents/{id}",
            "method": "GET",
            "targetSchema": {"$ref": "incident"}
          },
          {
            "rel": "closeIncident",
            "title": "Close an incident",
            "href": "/incidents/{id}/closeIncident",
            "method": "POST"
          },
          {
            "rel": "delete",
            "title": "Delete an incident",
            "href": "/incidents/{id}",
            "method": "DELETE"
          },
          {
            "rel": "edit",
            "title": "Update an incident",
            "href": "/incidents/{id}",
            "method": "PUT",
            "mediaType": "application/json",
            "schema": {"$ref": "patch-incident"},
            "targetSchema": {"$ref": "incident"}
          },
          {
            "rel": "parent",
            "title": "Incidents",
            "href": "/incidents",
            "method": "GET",
            "mediaType": "application/json",
            "targetSchema": {"$ref": "incidents"}
          }
        ]
      }

  - new-incident: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "new-incident",
        "title": "New Incident",
        "type": "object",
        "properties": {
          "problem": {
             "type": "string",
             "description": "Short description of the incident"
          },
          "description": {
             "type": "string",
             "description": "Detailed description of the incident"
          },
          "priority": {
             "type": "string",
             "description": "Incident priority: 1=high, 2=normal, 3=low",
             "enum": [
                "low",
                "normal",
                "high"
             ]
          },
          "category": {
             "type": "string",
             "description": "Incident category: appliance, electrical, general, heatingcooling, plumbing",
             "enum": [
                "appliance",
                "electrical",
                "general",
                "heatingcooling",
                "plumbing"
             ]
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "required": ["problem"],
        "additionalProperties": false
      }

  - patch-incident: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "patch-incident",
        "title": "Update Incident",
        "type": "object",
        "properties": {
          "problem": {
             "type": "string",
             "description": "Short description of the incident"
          },
          "description": {
             "type": "string",
             "description": "Detailed description of the incident"
          },
          "picture": {
            "type": "string",
            "media": {
                "binaryEncoding": "base64",
                "type": "image/png"
            },
            "description": "A picture uploaded to the incident activity"
          },
          "status": {
             "type": "string",
             "description": "Incident status: 1=open, 2=accepted, 3=fixed",
             "enum": [
                "open",
                "accepted",
                "fixed"
             ]
          },
          "priority": {
             "type": "string",
             "description": "Incident priority: 1=high, 2=normal, 3=low",
             "enum": [
                "low",
                "normal",
                "high"
             ]
          },
          "category": {
             "type": "string",
             "description": "Incident category: appliance, electrical, general, heatingcooling, plumbing",
             "enum": [
                "appliance",
                "electrical",
                "general",
                "heatingcooling",
                "plumbing"
             ]
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "minProperties": 1,
        "additionalProperties": false
      }

  - incident-customer: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "incident-customer",
        "title": "Customer Reporting Incident",
        "type": "object",

        "allOf": [
            {"$ref": "person"}
        ],
        "properties": {
          "id": {
             "type": "string",
             "description": "The customer ID"
          },
          "home": {
            "type": "string",
            "description": "The customer's home phone number"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "required": ["username", "firstName", "lastName"],
        "links": [
          {
            "rel": "canonical",
            "title": "Customer Details",
            "href": "/customers/{id}",
            "method": "GET",
            "targetSchema": {"$ref": "customer"}
          }
        ]
      }

  - IncidentLocation: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "IncidentLocation",
        "title": "Incident Location",
        "description": "The physical address where the incident occurred",
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Location Identifier"
          },
          "formattedAddress": {
            "type": "string",
            "description": "Formatted address"
          },
          "latitude": {
             "type": "string",
             "description": "The y coordinate (latitude) of the location point data"
          },
          "longitude": {
             "type": "string",
             "description": "The x coordinate (longitude) of the location point data"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "links": [
          {
            "rel": "canonical",
            "title": "Geographical Location",
            "href": "/locations/{id}"
          }
        ]
      }

  - incident-activities: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "incident-activities",
        "title": "Incident Activities",
        "type": "array",
        "description": "A list of incident activities, both system and user generated",
        "items": {
          "$ref": "incident-activity"
        },
        "links": [
          {
            "rel": "self",
            "title": "Incident Activities",
            "href": "/incidents/{id}/activities",
            "method": "GET",
            "targetSchema": {"$ref": "incident-activities"}
          },
          {
            "rel": "create",
            "title": "Incident Activity",
            "href": "/incidents/{id}/activities",
            "method": "POST",
            "encType": "application/x-www-form-urlencoded",
            "schema": {
              "properties": {
                "comment": {
                    "type": "string",
                    "description": "A brief comment posted by the user",
                    "maxLength": "100"
                },
                "picture": {
                  "type": "string",
                  "media": {
                      "binaryEncoding": "base64",
                      "type": "image/png"
                  },
                  "description": "A picture uploaded to the incident activity"
                },
                "revNum": {
                  "type": "integer",
                  "description": "Attribute used for offline cases and incremented when data is updated on the server"
                }
              }
            },
            "targetSchema": {"$ref": "incident-activity"}
          }
        ]
      }

  - incident-activity: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "incident-activity",
        "title": "Incident Activity",
        "type": "object",
        "description": "A single activity reported on an incident",
        "properties": {
            "id": {
               "type": "string",
               "description": "Unique incident activity identifier"
            },
            "firstName": {
               "type": "string",
               "description": "The first name of the person who created the activity"
            },
            "lastName": {
               "type": "string",
               "description": "The person's last name"
            },
            "userId": {
               "type": "string",
               "description": "The person's unique identifier"
            },
            "role": {
               "type": "string",
               "description": "The person's role, e.g. Customer or Field Service"
            },
            "comment": {
                "type": "string",
                "description": "A brief comment posted by the user",
                "maxLength": 100
            },
            "createdOn": {
                "type": "string",
                "format": "date-time",
                "description": "Date when the activity was created"
            },
            "picture": {
              "type": "string",
              "media": {
                  "binaryEncoding": "base64",
                  "type": "image/png"
              },
              "description": "A picture uploaded to the incident activity"
            },
            "revNum": {
              "type": "integer",
              "description": "Attribute used for offline cases and incremented when data is updated on the server"
            }
        }
      }

  - patch-incident-activity: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "patch-incident-activity",
        "title": "Update Incident Activity",
        "type": "object",
        "description": "A single activity reported on an incident",
        "properties": {
          "comment": {
              "type": "string",
              "description": "A brief comment posted by the user",
              "maxLength": 100
          },
          "picture": {
            "type": "string",
            "media": {
                "binaryEncoding": "base64",
                "type": "image/png"
            },
            "description": "A picture uploaded to the incident activity"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        }
      }

  - location: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "location",
        "title": "Geographical Location",
        "description": "The physical address of the location along with the geo coordinates",
        "type": "object",

        "properties": {
          "id": {
            "type": "string",
            "description": "A unique location ID identifying a single address"
          },
          "formattedAddress": {
            "type": "string",
            "description": "The fully formatted address"
          },
          "street1": {
            "type": "string",
            "description": "Street line 1"
          },
          "street2": {
            "type": "string",
            "description": "Street line 2"
          },
          "city": {
            "type": "string",
            "description": "City"
          },
          "state": {
            "type": "string",
            "description": "State"
          },
          "zip": {
            "type": "string",
            "description": "Zip"
          },
          "country": {
            "type": "string",
            "description": "Country"
          },
          "latitude": {
             "type": "string",
             "description": "The y coordinate (latitude) of the location point data"
          },
          "longitude": {
             "type": "string",
             "description": "The x coordinate (longitude) of the location point data"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "links": [
          {
            "rel": "self",
            "title": "Geographical Location",
            "href": "/locations/{id}"
          }
        ]
      }

  - customers: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "customers",
        "title": "Customers",
        "type": "object",
        "properties": {
          "count": {
             "type": "integer",
             "description": "The number of customers returned in the response"
          },
          "result": {
             "type": "array",
             "items": {
                "$ref": "customer"
             },
             "description": "The list of customers"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "links": [
          {
            "rel": "self",
            "title": "Customers",
            "href": "/customers",
            "method": "GET",
            "targetSchema": {"$ref": "customers"}
          }
        ]
      }

  - person: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "person",
        "title": "Person",
        "type": "object",
        "description": "A person object that includes proeprties common to any person",
        "properties": {
          "username": {
             "type": "string",
             "description": "The username of the person"
          },
          "firstName": {
             "type": "string",
             "description": "The person's first name"
          },
          "lastName": {
             "type": "string",
             "description": "The person's last name"
          },
          "email": {
             "type": "string",
             "format": "email",
             "description": "The person's email address"
          },
          "mobile": {
            "type": "string",
            "description": "The person's mobile phone number"
          },
          "photo":{
            "type": "string",
            "media": {
                "binaryEncoding": "base64",
                "type": "image/png"
            },
            "description": "A photo of the person"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "required": ["username", "firstName", "lastName"],
        "additionalProperties": true
      }

  - customer: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "customer",
        "title": "Customer",
        "type": "object",
        "allOf": [
            {"$ref": "person"}
        ],
        "properties": {
          "id": {
             "type": "string",
             "description": "The customer ID"
          },
          "address": {
            "$ref": "location",
            "description": "The primary address of the customer"
          },
          "home": {
            "type": "string",
            "description": "The customer's home phone number"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "required": ["id", "username", "firstName", "lastName", "mobile", "email"],
        "links": [
          {
            "rel": "self",
            "title": "Customer",
            "href": "/customers/{id}",
            "method": "GET",
            "targetSchema": {"$ref": "customer"}
          }
        ]

      }

  - new-customer: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "new-customer",
        "title": "New Customer",
        "type": "object",
        "properties": {
          "firstName": {
             "type": "string",
             "description": "The person's first name"
          },
          "lastName": {
             "type": "string",
             "description": "The person's last name"
          },
          "email": {
             "type": "string",
             "format": "email",
             "description": "The person's email address"
          },
          "home": {
            "type": "string",
            "description": "The customer's home phone number"
          },
          "mobile": {
            "type": "string",
            "description": "The person's mobile phone number"
          },
          "photo":{
            "type": "string",
            "media": {
                "binaryEncoding": "base64",
                "type": "image/png"
            },
            "description": "A photo of the person"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "required": ["firstName", "lastName"],
        "additionalProperties": false
      }

  - patch-customer: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "patch-customer",
        "title": "Update Customer",
        "type": "object",
        "allOf": [
            {"$ref": "person"}
        ],
        "properties": {
          "address": {
            "$ref": "location",
            "description": "The primary address of the customer"
          },
          "home": {
            "type": "string",
            "description": "The customer's home phone number"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "minProperties": 1,
        "additionalProperties": false
      }

  - technician: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "technician",
        "title": "Technician Profile",
        "allOf": [
            {"$ref": "person"}
        ],
        "type": "object",
        "properties": {
          "id": {
             "type": "number",
             "description": "The technician ID"
          },
          "role": {
            "type": "string",
            "enum": ["Field Service", "Online Help"],
            "description": "the role of technician"
          },
          "location": {
            "$ref": "location",
            "description": "The primary location of the technician"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "required": ["id", "username", "firstName", "lastName", "mobile", "email", "role"],
        "links": [
          {
            "rel": "self",
            "title": "Technician",
            "href": "/users/~",
            "method": "GET",
            "targetSchema": {"$ref": "technician"}
          }
        ]
      }

  - patch-technician: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "patch-technician",
        "allOf": [
            {"$ref": "person"}
        ],
        "title": "Update Technician",
        "type": "object",
        "properties": {
          "role": {
            "type": "string",
            "enum": ["Field Service", "Online Help"],
            "description": "role of technician"
          },
          "revNum": {
            "type": "integer",
            "description": "Attribute used for offline cases and incremented when data is updated on the server"
          }
        },
        "minProperties": 1,
        "additionalProperties": false
      }

  - stats: |
      {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "id": "stats",
        "title": "Analytics",
        "type": "object",
        "properties": {
          "metrics": {
            "type": "array",
            "description": "Metrics are individual measurements related to incident activity, techncian performance",
            "items": {
              "type": "object",
              "properties": {
                "month": {
                  "type": "string",
                  "description": "Date Dimension for which a data point is provided"
                },
                "technician": {
                  "type": "string",
                  "description": "Technician for whom the data is provided"
                },
                "radius": {
                  "type": "number",
                  "description": "Radius in miles from the technician location, where incidents were reported"
                },
                "incidentsAssigned": {
                  "type": "number",
                  "description": "Incidents Assigned to Technician"
                },
                "incidentsClosed": {
                  "type": "number",
                  "description": "Incidents Closed by Technician"
                },
                "revNum": {
                  "type": "integer",
                  "description": "Attribute used for offline cases and incremented when data is updated on the server"
                }
              }
            }
          }
        }
      }

  - incidentsStats: |
      {
